{
    "name": "api-node",
    "version": "1.0.0",
    "description": "",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "watch-ts": "tsc -w",
        "watch-node": "nodemon ./dist/index.js",
        "serve": "concurrently \"tsc -w --extendedDiagnostics\" \"nodemon -L ./dist/index.js\"",
        "stagging": "tsc && node ./dist/index.js",
        "start": "nodemon -L ./src/index.ts",
        "lint:check": "prettier --config ./.prettierrc --check \"src/**/*.ts\"",
        "lint:format": "npx prettier --write .",
        "test": "mocha -r ts-node/register src/**/*.spec.ts",
        "coverage": "nyc npm run test",
        "make-migrate": "npx knex migrate:make -x ts",
        "migrate": "npx knex migrate:latest --env",
        "migrate-rollback": "npx knex migrate:rollback --env",
        "make-seed": "npx knex seed:make",
        "seed": "npx knex seed:run"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/app.iness/api-node.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/app.iness/api-node/issues"
    },
    "homepage": "https://gitlab.com/app.iness/api-node#readme",
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/chai-http": "^4.2.0",
        "@types/colors": "^1.2.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^26.0.24",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/knex": "^0.16.1",
        "@types/mocha": "^8.2.3",
        "@types/morgan": "^1.9.3",
        "@types/node": "^15.14.9",
        "@types/nodemailer": "^6.4.4",
        "@types/pg": "^8.6.4",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "concurrently": "^6.5.1",
        "eslint": "^7.32.0",
        "mocha": "^9.2.1",
        "nodemon": "^2.0.15",
        "nyc": "^15.1.0",
        "password-hash": "^1.2.2",
        "prettier": "^2.5.1",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "axios": "^0.26.0",
        "body-parser": "^1.19.2",
        "colors": "^1.4.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.3",
        "express-group-routes": "^1.1.0",
        "https": "^1.0.0",
        "indicative": "^7.4.4",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.15",
        "morgan": "^1.10.0",
        "nodemailer": "^6.7.2",
        "pg": "^8.7.3",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0",
        "uuid": "^3.4.0"
    }
